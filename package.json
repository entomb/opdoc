{
  "name": "opdoc",
  "version": "0.1.17",
  "description": "One Page Documentation",
  "author": "Jonathan Tavares @entomb",
  "bin": {
    "opdoc": "./bin/run"
  },
  "homepage": "https://github.com/entomb/opdoc",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "git@github.com:entomb/opdoc.git",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.1",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/marked": "^4.0.7",
    "@types/node-emoji": "^1.8.1",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "globby": "^11",
    "html-minifier-terser": "^7.0.0-beta.0",
    "marked": "^4.1.1",
    "node-emoji": "^1.11.0",
    "prismjs": "^1.29.0",
    "remove": "^0.1.5",
    "sanitize-html": "^2.7.2",
    "slugify": "^1.6.5",
    "toc": "^0.4.0",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.2",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "opdoc",
    "dirname": "opdoc",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && shx cp -r src/template dist/template",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/entomb/opdoc/issues",
  "keywords": [
    "oclif",
    "markdown",
    "documentation"
  ],
  "types": "dist/index.d.ts"
}
